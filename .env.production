# Frontend Environment Configuration for Production
# یہ file build کے وقت استعمال ہوگی

# ═══════════════════════════════════════════════════════════
# API Configuration
# ═══════════════════════════════════════════════════════════

VITE_API_BASE_URL=https://p22dbd8vui.us-east-2.awsapprunner.com/api
VITE_API_TIMEOUT=30000

# ═══════════════════════════════════════════════════════════
# App Configuration
# ═══════════════════════════════════════════════════════════

VITE_APP_NAME=ContractGuard
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production
VITE_DEBUG=false

# ═══════════════════════════════════════════════════════════
# AWS Configuration (Frontend سے استعمال)
# ═══════════════════════════════════════════════════════════

VITE_AWS_REGION=us-east-1
VITE_S3_BUCKET=janta-contractguard

# ═══════════════════════════════════════════════════════════
# Feature Flags
# ═══════════════════════════════════════════════════════════

VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_DARK_MODE=true

# ═══════════════════════════════════════════════════════════
# Authentication
# ═══════════════════════════════════════════════════════════

VITE_AUTH_PROVIDER=jwt
VITE_JWT_STORAGE=localStorage

# ═══════════════════════════════════════════════════════════
# External Services (اگر ضروری ہو)
# ═══════════════════════════════════════════════════════════

